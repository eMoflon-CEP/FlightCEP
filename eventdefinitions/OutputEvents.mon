event RequestCompletionEvent {
	integer messageID;
}

event CompletionEvent {
	integer messageID;
	string reason;
}

event SendWorkingConnectingFlights {
	integer messageID;
	integer workingConnectingFlights;
}

event SendDelayedConnectingFlights {
	integer messageID;
	integer delayedConnectingFlights;
}

event Issue {
	integer issueMatchID;
	string description;
}

event RemoveIssue {
	integer issueMatchID;
}

event Solution {
	integer solutionMatchID;
	integer issueMatchID;
	string description;
}

event RemoveSolution {
	integer solutionMatchID;
	integer issueMatchID;
}
